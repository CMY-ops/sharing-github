    "try:\n",
    "\n",
    "\n",
    "    if Levered_Free_Cash_Flow[-1] == 'T':\n",
    "        FCF_str = Levered_Free_Cash_Flow[:-1]\n",
    "\n",
    "        FCF_float = float(FCF_str)\n",
    "\n",
    "        multiplied_FCF_num = FCF_float * 1000000\n",
    "\n",
    "    elif Levered_Free_Cash_Flow[-1] == 'B':\n",
    "        FCF_str = Levered_Free_Cash_Flow[:-1]\n",
    "\n",
    "        FCF_float = float(FCF_str)\n",
    "\n",
    "        multiplied_FCF_num = FCF_float * 1000\n",
