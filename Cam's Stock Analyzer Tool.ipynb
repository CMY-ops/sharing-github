    "    quarterly_ten_yr_whole_df.set_index('Date',inplace=True)\n",
    "\n",
    "except:\n",
    "    print(\"Failed to gather Quarterly 10 Year Data\")\n",
    "    pass\n",
    "\n",
    "\n",
    "# Fair Value Calculation:\n",
    "\n",
    "try:\n",
    "    fair_value = float(format(round(((EPS_TTM * Rounded_Avg_5yr_PE *(1 + (YF_5_Yr_Growth_Estimate/100))**10) / (1 + (req_rate/100))**10) , 2)))\n",
    "except:\n",
    "    print(\"Error:                  Fair Value Incalculable\")\n",
    "\n",
    "    # Update 11/17/2022 ... if fair vaule is not found, default value will be \"1\".\n",
    "    # This allows companies that pass \"8 pillars valuation\" to be written to 'Cam's Fair Values.txt\"\n",
    "    # Even if the companies that pass that test do not return a fair value amount!\n",
    "\n",
    "    fair_value = 1\n",
    "    pass\n",
    "\n",
    "try:\n",
    "    if fair_value > current_price:\n",
    "        disc_to_fv = round(  ((1-(current_price/fair_value) )*100) , 2 )\n",
    "    elif fair_value < current_price: \n",
    "        disc_to_fv = 0\n",
    "except:\n",
    "    print(\"Error:                  Discount to Fair Value Incalculable\")      \n",
    "\n",
    "\n",
    "\n",
    "# Buy, Sell or Hold?\n",
    "\n",
    "\n",
    "\n",
    "try:\n",
    "    buy_sell_or_hold = 'UNKNOWN'\n",
    "    if current_price < fair_value:\n",
    "        buy_sell_or_hold = 'UNDERVALUED'\n",
    "\n",
    "    elif current_price == fair_value:\n",
    "        buy_sell_or_hold = 'AT FAIR VALUE'\n",
    "\n",
    "    elif current_price > fair_value:\n",
    "        buy_sell_or_hold = 'OVERVALUED'\n",
    "\n",
    "except:\n",
    "    print(\"Error:                  Unable to Determine: Buy, Sell or Hold\")\n",
    "    pass\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "# Added 06/09/2022: Dividend Payout Ratio & Dividend Coverage Ratio \n",
    "\n",
    "\n",
    "try:\n",
    "    dividend_payout_ratio = round(((current_dividend_amount/EPS_TTM)*100),2)\n",
    "\n",
    "    # 30-50% is considered healthy, while anything over 50% could be unsustainable.\n",
    "\n",
    "except:\n",
    "    dividend_payout_ratio = \"Unavailable\"\n",
